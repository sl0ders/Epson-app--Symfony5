{% trans_default_domain "EpsonProjectTrans" %}
<div class="card-body">
    <div class="row">
        <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="ibox float-e-margins col-md-12">
                    <div class="ibox-title">
                        <h5 class="text-black-50">{{ "printer.title.stats"|trans({'%printName%': printer.name}) }}</h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"> </i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="papersContainer">
                            <div id="div_chart_paper"></div>
                        </div>
                        <div class="papersContainer">
                            <div id="div_chart_ink"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
